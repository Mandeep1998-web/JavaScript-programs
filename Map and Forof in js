var sam={
    name:'my name is sam',
    age:24,
    isActive:true
}
var mile={
    name:"my name is mile ",
    age:25,
    isActive:true

}
var milind={
    name:'my name is milind',
    age:29,
    isActive:false
}
let users = new Map ()
users.set('sam',sam) // it is basically the key value pair// so here the sam in string is key and other is variable that we are pretended
users.set('mile',mile)
users.set('milind',milind)
// console.log(users.values('key'))   
// map is different from the object because in map we can know the size by using size property but not in the case  of object

